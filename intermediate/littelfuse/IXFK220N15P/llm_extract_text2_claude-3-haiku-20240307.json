    "advantages": [
      "High Power Density",
      "Easy to Mount",
      "Space Savings"
    ],
    "applications": [
      "DC-DC Converters",
      "Battery Chargers",
      "Switch-Mode and Resonant-Mode Power Supplies",
      "DC Choppers",
      "AC and DC Motor Drives",
      "Uninterrupted Power Supplies",
      "High Speed Power Switching Applications"
    ]
  },
